DROP TABLE KULLANICITABLOSU;


CREATE TABLE SELLER(
    ID int,
    FULL_NAME varchar(64),
    EMAIL varchar(96),
    PASSWORD varchar(255),
    PHONE varchar(32),
    ADDRES_ID int,
    FOOD_ID int,
    PRIMARY KEY (ID)
);

CREATE TABLE FOOD(
    ID int,
    NAME varchar(32),
    DESCRIPTION varchar(32),
    FOOD_TYPE_ID varchar(32),
    PRIMARY KEY (ID)
);

CREATE TABLE ADDRES(
    ID int,
    NAME varchar(32),
    COUNTRY varchar(32),
    CITY  varchar(32),
    DISTRICT varchar(32),
    NEIGHBOURHOOD varchar(32),
    DETAIL varchar(255),
    PRIMARY KEY (ID)
);

CREATE TABLE FOOD_TYPE(
    ID int,
    NAME varchar(32),
    PRIMARY KEY (ID)
);

CREATE TABLE KullaniciTablosu (
    Kullanici_id int,
    soyadi varchar(32),
    isim varchar(32),
    adres varchar(255),
    sehir varchar(32),
    PRIMARY KEY (Kullanici_id)
);

CREATE TABLE BOOK (
    ID int,
    NAME varchar(32),
    YAZAR varchar(32),
    KULLANICI_ID int,
    PRIMARY KEY (ID)
);

